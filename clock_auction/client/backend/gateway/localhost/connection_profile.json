{
    "name": "basic-network",
    "version": "1.0.0",
    "client": {
        "organization": "Org1",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "500"
                },
                "orderer": "500"
            }
        }
    },
    "channels": {
        "mychannel": {
            "orderers": [
                "orderer.example.com"
            ],
            "peers": {
                "peer0.org1.example.com": {}
            }
        }
    },
    "organizations": {
        "Org1": {
            "mspid": "Org1MSP",
            "peers": [ "peer0.org1.example.com" ],
            "certificateAuthorities": [ "ca.example.com" ]

        }
    },
    "orderers": {
        "orderer.example.com": {
            "url": "grpc://orderer.example.com:7050"
        }
    },
    "peers": {
        "peer0.org1.example.com": {
            "url": "grpc://peer0.org1.example.com:7051"
        }
    },

    "certificateAuthorities": {
        "ca.example.com": {
            "unused2-url": "http://localhost:7054",
            "unused1-home-url": "http://192.168.0.6:7054",
            "unused-nov26-url": "http://9.24.25.103:7054",
            "url": "http://ca.example.com:7054",
            "unused-httpOptions": { "verify": false },
            "registrar": {
                "enrollId": "admin",
                "enrollSecret": "adminpw"
            }
        }
    }
}
